<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.3"
              xmlns="https://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://xmlns.jcp.org/xml/ns/javaee 
              https://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
<converter>
  <converter-for-class>java.lang.Enum</converter-for-class>
  <converter-class>javax.faces.convert.EnumConverter</converter-class>
</converter>
<navigation-rule>
    <from-view-id>/index.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>userHome</from-outcome>
        <to-view-id>/User/userHome.xhtml</to-view-id>
        <redirect></redirect>
    </navigation-case>
    <navigation-case>
        <from-outcome>adminHome</from-outcome>
        <to-view-id>/Admin/adminHome.xhtml</to-view-id>
        <redirect></redirect>
    </navigation-case>
    <navigation-case>
        <from-outcome>userRegister</from-outcome>
        <to-view-id>/userRegister.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/userRegister.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>success</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>error</from-outcome>
        <to-view-id>/userRegister.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/User/userHome.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>makePayment</from-outcome>
        <to-view-id>/User/userPay.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>requestPayment</from-outcome>
        <to-view-id>/User/userPaymentRequest.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>logout</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/User/userPay.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>success</from-outcome>
        <to-view-id>/User/userHome.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>error</from-outcome>
        <to-view-id>/User/userPay.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/User/userPaymentRequest.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>success</from-outcome>
        <to-view-id>/User/userHome.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>error</from-outcome>
        <to-view-id>/User/userPaymentRequest.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/User/userPaymentRequestDenied.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>home</from-outcome>
        <to-view-id>/User/userHome.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/Admin/adminHome.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>registerAdmin</from-outcome>
        <to-view-id>/Admin/adminRegister.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>index</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>logout</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/Admin/adminRegister.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>success</from-outcome>
        <to-view-id>/Admin/adminHome.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>error</from-outcome>
        <to-view-id>/Admin/adminRegister.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
</faces-config>